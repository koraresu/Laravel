	.section-wrap
		.service-box
			color: #fff;
			padding: 66px 25px;
			position: relative;
			text-align: center;
			transform: translateZ(0px);
			z-index: 1;
			margin-bottom: 30px;
			&:before,
			&::after
				bottom: 0;
				content: "";
				left: 0;
				margin: 10px;
				position: absolute;
				right: 0;
				top: 0;
				transition: all 0.3s ease 0s;
				z-index: 3;
			&::before
				border-left: 1px solid #fff;
				border-right: 1px solid #fff;
				transform: scaleY(0);
				transform-origin: 100% 0 0;
			&::after
				border-bottom: 1px solid #fff;
				border-top: 1px solid #fff;
				transform: scaleX(0);
				transform-origin: 0 100% 0;
			.service-box-overlay
				background-color: #37b4f0;
				bottom: 0;
				left: 0;
				position: absolute;
				right: 0;
				top: 0;
				z-index: 2;
				transition: all 0.3s ease 0s;
				background-color: #37b4f0;
				opacity: 0.9;
			& > i 
				color: #FFF;
				display: block;
				font-size: 64px;
				line-height: 64px;
				margin-bottom: 15px;
				position: relative;
				z-index: 4;
			.service-box-content
				position: relative;
				z-index: 4;
				a
					color: #FFF;
				h5
					text-transform: uppercase;
			&::before
				transform: scaleY(1);
			&::after
				transform: scaleX(1);
