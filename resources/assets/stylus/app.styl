	.emergencies
		position absolute
		top 0
		right 0
		z-index 2
		background-color #0d79aa
		padding 15px 30px
		&:before
			content ""
			position absolute
			top 0
			left 0
			bottom 0
			width 14px
			background-color #37b4f0
